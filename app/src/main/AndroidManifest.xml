<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.park">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/park_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/park_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FindResultList"></activity>
        <activity android:name=".FindResult" />
        <activity android:name=".Provide" />
        <activity android:name=".Find" />
        <activity android:name=".Welcome" />
        <activity android:name=".History" />
        <activity android:name=".Pending" />
        <activity android:name=".Verified" />
        <activity android:name=".Notifications" />
        <activity android:name=".ViewParkingSpace" />
        <activity android:name=".ParkingStatus" />
        <activity android:name=".ProfileDetails" />
        <activity android:name=".Profile" />
        <activity android:name=".Menu" />
        <activity android:name=".Registration" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>